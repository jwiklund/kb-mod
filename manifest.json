{
  "name": "Easy Browse",
  "version": "1.0",
  "description": "Easily browse known sites.",
  "content_scripts": [
    {
      "matches": ["http://xkcd.com/*"],
      "js": ["jquery-1.4.2.min.js", "xkcd.js", "common.js"]
    },
    {
      "matches": ["http://questionablecontent.net/*"],
      "js": ["jquery-1.4.2.min.js", "questionable.js", "common.js"]
    },
    {
      "matches": ["http://www.sinfest.net/*"],
      "js": ["jquery-1.4.2.min.js", "sinfest.js", "common.js"]
    }

  ]
}